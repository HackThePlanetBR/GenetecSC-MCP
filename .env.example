# Genetec Security Center Configuration
# Copy this file to .env and fill in your actual values

# Genetec Server URL (include protocol and port)
# Example: https://your-server.com:4590/WebSdk
GENETEC_SERVER_URL=https://localhost:4590/WebSdk

# Genetec Security Center username
GENETEC_USERNAME=admin

# Genetec Security Center password
GENETEC_PASSWORD=your_password_here

# SDK Application ID from your certificate
# For development/demo systems, use the standard SDK certificate
GENETEC_APP_ID=KxsD11z743Hf5Gq9mv3+5ekxzemlCiUXkTFY5ba1NOGcLCmGstt2n0zYE9NsNimv

# API request timeout in seconds (default: 30)
GENETEC_TIMEOUT=30

# SSL verification (set to false for self-signed certificates in dev)
# WARNING: Only disable SSL verification in development environments
GENETEC_VERIFY_SSL=true